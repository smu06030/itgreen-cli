{
  "webp": {
    "inputPath": "public/images",
    "outputPath": "public/webp",
    "quality": 80,
    "includePatterns": ["*.{png,jpg,jpeg,webp,PNG,JPG,JPEG,WEBP}"],
    "excludePatterns": ["**/node_modules/**"]
  },
  "genImg": {
    "inputPath": "test-images",
    "outputPath": "test-output/images.ts",
    "displayName": "TEST_IMAGES",
    "basePath": "/",
    "includingPattern": ["*.jpg", "*.png", "*.svg", "*.jpeg", "*.webp"],
    "ignoredPattern": ["*node_module*"]
  },
  "genRoute": {
    "inputPath": "test-routes/pages",
    "outputPath": "test-output/routes.ts",
    "displayName": "ROUTES",
    "ignoredPattern": [
      "layout.tsx",
      "loading.tsx",
      "error.tsx",
      "not-found.tsx",
      "template.tsx"
    ],
    "includingPattern": ["**/*.tsx"]
  }
}
